local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Centipede", "DarkTheme")

-- Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")

MainSection:NewButton("No Npc", "Will Remove Npx kind of", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/2567-rblx/scripts/main/Notoriety/RemoveNPCs.lua",true))()
end)

MainSection:NewButton("Inf Stamina", "Increase your stamina", function()
    -- Callback function to set stamina value
    local playerLocal = game.Players.LocalPlayer
    game.Workspace.Criminals[playerLocal.Name].Stamina.Value = 2 ^ 60
end)

MainSection:NewButton("Yell at All Citizens", "Yell at all citizens in the game", function()
    local citizens = workspace.Citizens
    local yellRemote = game:GetService('ReplicatedStorage').RS_Package.Remotes.PlayerYell
    yellRemote:FireServer(citizens:GetChildren())
end)

MainSection:NewButton("Camera Esp", "Shows you where cameras are", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ZirhyOG/BoostScript/refs/heads/main/h", true))();
end)

MainSection:NewButton("Tp bags to van", "Most maps work", function()
    for i,v in pairs(game:GetService("Workspace").Bags:GetDescendants()) do
        if v.Name == 'MoneyBag' then
        v.CFrame = game:GetService("Workspace").BagSecuredArea.FloorPart.CFrame
        end
        end
end)

--Misc
local Misc = Window:NewTab("Misc")
local MiscSection = Misc:NewSection("Misc")

MiscSection:NewButton("TP (Keycard)", "Teleports you to the keycard", function()
    -- Callback function for teleportation
    local playerLocal = game.Players.LocalPlayer
    for i, v in pairs(game.Workspace.Map:GetChildren()) do
        if v.Name == "KeyCard" then
            playerLocal.Character:MoveTo(v.KeyCard.Position)
        end
    end
end)

MiscSection:NewButton("Force Reset", "Vote Reset", function()
    game:GetService("ReplicatedStorage").RS_Package.Remotes.VoteReset:FireServer()
end)
